{
  "name": "wpbf-gutenberg",
  "version": "0.1.0",
  "description": "Gutenberg blocks for Page Builder Framework",
  "author": "David",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@swc/helpers": "^0.3.13",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.5.0",
    "parcel": "^2.5.0",
    "prettier": "npm:wp-prettier@^2.6.2-beta-1"
  },
  "alias": {
    "wp": {
      "global": "wp"
    },
    "react": {
      "global": "React"
    },
    "react-dom": {
      "global": "ReactDOM"
    },
    "@wordpress/blocks": {
      "global": "wp.blocks"
    },
    "@wordpress/block-editor": {
      "global": "wp.blockEditor"
    },
    "@wordpress/components": {
      "global": "wp.components"
    },
    "@wordpress/element": {
      "global": "wp.element"
    },
    "@wordpress/i18n": {
      "global": "wp.i18n"
    },
    "@wordpress/icons": {
      "global": "wp.icons"
    }
  },
  "scripts": {
    "watch": "parcel watch src/wpbf-block-editor.js --dist-dir build",
    "dev": "parcel build src/wpbf-block-editor.js --no-optimize --dist-dir build",
    "build": "parcel build src/wpbf-block-editor.js --dist-dir build"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ]
}
