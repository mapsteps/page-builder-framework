{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 2,
	"name": "wpbf/notices",
	"title": "Notices",
	"category": "wpbf",
	"icon": "info-outline",
	"description": "Notice block utilizing CSS Framework of Page Builder Framework.",
	"textdomain": "page-builder-framework",
	"supports": {
		"anchor": true
	},
	"attributes": {
		"contentJustification": {
			"type": "string",
			"default": "left"
		},
		"orientation": {
			"type": "string",
			"default": "horizontal"
		},
		"id": {
			"type": "string"
		}
	}
}